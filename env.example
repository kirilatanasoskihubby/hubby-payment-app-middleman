# Copy this file to .env and update the values

# Port for the middleman service
PORT=3000

# URL of the target service to forward requests to
TARGET_SERVICE_URL=http://localhost:4000

# Rate limiting configuration (Stripe-like limits)
# Time window in minutes for rate limiting (default: 1 minute)
RATE_LIMIT_WINDOW_MINUTES=1

# Maximum requests allowed per time window (default: 100, similar to Stripe's ~100 RPS)
RATE_LIMIT=100
